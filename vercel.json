{
    "rewrites": [
        {
            "source": "/api/media.json",
            "destination": "/public/api/media.json"
        },
        {
            "source": "/api/schedule.json",
            "destination": "/public/api/schedule.json"
        },
        {
            "source": "/api/menu.json",
            "destination": "/public/api/menu.json"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}